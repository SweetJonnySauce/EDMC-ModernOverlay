{
  "BGS-Tally": {
    "__match__": {
      "id_prefixes": [
        "bgstally-"
      ]
    },
    "notes": [
      "Everything for bgstally"
    ],
    "grouping": {
      "groups": {
        "colonisation": {
          "id_prefixes": [
            "bgstally-msg-colonisation-",
            "bgstally-frame-colonisation"
          ]
        },
        "tick-notifications": {
          "id_prefixes": [
            "bgstally-frame-tick",
            "bgstally-msg-tick-0",
            "bgstally-frame-system_tick",
            "bgstally-msg-system_tick-0",
            "bgstally-msg-tickwarn-",
            "bgstally-frame-tickwarn"
          ]
        }
      }
    }
  },
  "ED-Mining-Analytics": {
    "grouping": {
      "groups": {
        "All-edmca-prefixes": {
          "id_prefixes": [
            "edmcma"
          ],
          "anchor": "nw"
        }
      }
    }
  },
  "EDR": {
    "__match__": {
      "id_prefixes": [
        "edr-"
      ]
    },
    "grouping": {
      "groups": {
        "docking": {
          "id_prefixes": [
            "edr-docking-",
            "edr-docking-station-"
          ]
        },
        "bounty": {
          "id_prefixes": [
            "edr-bounty"
          ]
        },
        "intel": {
          "id_prefixes": [
            "edr-intel"
          ]
        },
        "target": {
          "id_prefixes": [
            "edr-target"
          ]
        },
        "notice": {
          "id_prefixes": [
            "edr-notice-"
          ]
        }
      }
    }
  },
  "LandingPad": {
    "notes": [
      "This groups the LandingPad overlay so Fill mode can handle the image as a single unit."
    ],
    "__match__": {
      "id_prefixes": [
        "shell-",
        "line-",
        "toaster-left-",
        "toaster-right-",
        "pad-",
        "station-"
      ]
    },
    "grouping": {
      "groups": {
        "All LandingPad Prefixes": {
          "id_prefixes": [
            "shell-",
            "line-",
            "toaster-left-",
            "toaster-right-",
            "pad-",
            "station-"
          ],
          "anchor": "nw"
        }
      }
    }
  }
}
