{
  "LandingPad": {
    "__match__": {
      "id_prefixes": [
        "shell-",
        "line-",
        "toaster-left-",
        "toaster-right-",
        "pad-"
      ]
    },
    "shell-*": {
      "x_scale": 2.0
    },
    "line-*": {
      "x_scale": 2.0,
      "x_shift": {
        "mode": "align_center",
        "target": 200.0
      }
    },
    "toaster-left-*": {
      "x_scale": 2.0
    },
    "toaster-right-*": {
      "x_scale": 2.0
    },
    "pad-*": {
      "x_scale": 2.0
    }
  }
}